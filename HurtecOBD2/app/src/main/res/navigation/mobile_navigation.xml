<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.hurtec.obd2.diagnostics.ui.dashboard.DashboardFragment"
        android:label="@string/nav_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_diagnostics"
        android:name="com.hurtec.obd2.diagnostics.ui.diagnostics.DiagnosticsFragment"
        android:label="@string/nav_diagnostics"
        tools:layout="@layout/fragment_diagnostics" />

    <fragment
        android:id="@+id/navigation_data"
        android:name="com.hurtec.obd2.diagnostics.ui.data.DataFragment"
        android:label="@string/nav_data"
        tools:layout="@layout/fragment_data" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.hurtec.obd2.diagnostics.ui.settings.SettingsFragment"
        android:label="@string/nav_settings"
        tools:layout="@layout/fragment_settings" />

</navigation>
